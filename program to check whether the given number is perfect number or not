class HelloWorld {
    public static void main(String[] args) {
        int num = 496, sum = 0;
        for(int i = 1; i<num ; i++){
            if(num % i == 0){
                sum += i;
            }
            
        }
        if(sum == num){
            System.out.println("the number is a perfect number");
        }
        else{ 
            System.out.println("the number is not perfect number");
            
    }
}
}
