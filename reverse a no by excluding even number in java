
import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        int rev = 0;
        Scanner sc = new Scanner(System.in);
        int number = sc.nextInt();
        while(number != 0){
            int remainder = number % 10;
            if(remainder % 2 != 0){
              rev  = rev * 10 + remainder;
             number = number/10;
              
            }
            else {
             rev  = rev;
             number = number/10;
            
           
            
            }
        
        
    }
    System.out.println(rev);
}
}
